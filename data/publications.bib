	keywords = {genetic association studies, Journal Article, genetics, cardiomyopathy, dilated, connectin, continental population groups},
	pages = {42--54},
	file = {Full Text:/Users/mglevin/Zotero/storage/ZFZQT562/Haggerty et al. - 2019 - Genomics-First Evaluation of Heart Disease Associa.pdf:application/pdf},
}

@article{doi:10.1161/CIRCGEN.118.002352,
