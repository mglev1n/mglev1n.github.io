	pmid = {39549270},
	pmcid = {PMC11569413},
	keywords = {Genetic Predisposition to Disease, Genotype, Humans, Multifactorial Inheritance, Female, Male, Adult, Aged, Middle Aged, Coronary Disease, Risk Assessment, Cross-Sectional Studies, Genetic Risk Score, Area Under Curve, ROC Curve},
	pages = {60--70},
	file = {Full Text PDF:/Users/mglevin/Zotero/storage/UBNHVZ4J/Abramowitz et al. - 2025 - Evaluating Performance and Agreement of Coronary Heart Disease Polygenic Risk Scores.pdf:application/pdf;Snapshot:/Users/mglevin/Zotero/storage/LXL4MXP7/2826347.html:text/html},
}

