	note = {Pages: 2023.07.30.23293390},
	file = {Full Text PDF:/Users/mglevin/Zotero/storage/J65Q7NIA/Adkar et al. - 2023 - Dissecting the Genetic Architecture of Intracrania.pdf:application/pdf},
}

