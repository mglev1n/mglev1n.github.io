	year = {2021},
	pmid = {35186008},
	pmcid = {PMC8847690},
	keywords = {peripheral artery disease, atherosclerosis, Journal Article, pleiotropy, GWAS—genome-wide association study, multi-trait analyses},
	pages = {787545},
	file = {Full Text:/Users/mglevin/Zotero/storage/H6JF8WTQ/Bellomo et al. - 2021 - Multi-Trait Genome-Wide Association Study of Ather.pdf:application/pdf;Snapshot:/Users/mglevin/Zotero/storage/T5TIPWI6/Bellomo et al. - 2021 - Multi-trait GWAS of atherosclerosis detects novel .html:text/html},
